Class IntegerPal(PalindromeChecker):
    def palindrome(self,input_value ):
        input_str=str(input_value)
        return input_str=input_str [::-1]
        
input_str= input("Efnter # string:")

string_checker=StringPal()

if string_checker.palindrome input_str):
    print("The input string is a palindrome.")
else
    print("The input string is not palindrome.")
    
